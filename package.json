{
  "name": "raffel",
  "version": "0.2.2",
  "description": "Unified multi-protocol server runtime. One core, multiple transports.",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "raffel-mcp": "./dist/mcp/cli.js"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./stream": {
      "types": "./dist/stream/index.d.ts",
      "import": "./dist/stream/index.js"
    },
    "./validation/zod": {
      "types": "./dist/validation/adapters/zod.d.ts",
      "import": "./dist/validation/adapters/zod.js"
    },
    "./validation/yup": {
      "types": "./dist/validation/adapters/yup.d.ts",
      "import": "./dist/validation/adapters/yup.js"
    },
    "./validation/joi": {
      "types": "./dist/validation/adapters/joi.d.ts",
      "import": "./dist/validation/adapters/joi.js"
    },
    "./validation/ajv": {
      "types": "./dist/validation/adapters/ajv.d.ts",
      "import": "./dist/validation/adapters/ajv.js"
    },
    "./validation/fastest": {
      "types": "./dist/validation/adapters/fastest.d.ts",
      "import": "./dist/validation/adapters/fastest.js"
    },
    "./mcp": {
      "types": "./dist/mcp/index.d.ts",
      "import": "./dist/mcp/index.js"
    },
    "./docs/ui": {
      "types": "./dist/ui/index.d.ts",
      "import": "./dist/ui/index.js"
    },
    "./middleware/http": {
      "types": "./dist/middleware/http/index.d.ts",
      "import": "./dist/middleware/http/index.js"
    },
    "./usd": {
      "types": "./dist/usd/index.d.ts",
      "import": "./dist/usd/index.js"
    },
    "./http": {
      "types": "./dist/http/index.d.ts",
      "import": "./dist/http/index.js"
    },
    "./http/cookie": {
      "types": "./dist/http/cookie.d.ts",
      "import": "./dist/http/cookie.js"
    },
    "./http/cors": {
      "types": "./dist/http/cors.d.ts",
      "import": "./dist/http/cors.js"
    },
    "./http/compress": {
      "types": "./dist/http/compress.d.ts",
      "import": "./dist/http/compress.js"
    },
    "./http/security": {
      "types": "./dist/http/security.d.ts",
      "import": "./dist/http/security.js"
    },
    "./http/response": {
      "types": "./dist/http/response.d.ts",
      "import": "./dist/http/response.js"
    },
    "./http/errors": {
      "types": "./dist/http/errors.d.ts",
      "import": "./dist/http/errors.js"
    },
    "./http/health": {
      "types": "./dist/http/health.d.ts",
      "import": "./dist/http/health.js"
    },
    "./http/guards": {
      "types": "./dist/http/guards.d.ts",
      "import": "./dist/http/guards.js"
    },
    "./http/auth": {
      "types": "./dist/http/auth.d.ts",
      "import": "./dist/http/auth.js"
    },
    "./http/body-limit": {
      "types": "./dist/http/body-limit.d.ts",
      "import": "./dist/http/body-limit.js"
    },
    "./http/static": {
      "types": "./dist/http/static.d.ts",
      "import": "./dist/http/static.js"
    },
    "./http/banner": {
      "types": "./dist/http/banner.d.ts",
      "import": "./dist/http/banner.js"
    },
    "./http/events": {
      "types": "./dist/http/events.d.ts",
      "import": "./dist/http/events.js"
    },
    "./http/failban": {
      "types": "./dist/http/failban.d.ts",
      "import": "./dist/http/failban.js"
    },
    "./http/session": {
      "types": "./dist/http/session.d.ts",
      "import": "./dist/http/session.js"
    },
    "./http/rate-limit": {
      "types": "./dist/http/rate-limit.d.ts",
      "import": "./dist/http/rate-limit.js"
    },
    "./http/validate": {
      "types": "./dist/http/validate.d.ts",
      "import": "./dist/http/validate.js"
    },
    "./http/templates": {
      "types": "./dist/http/templates.d.ts",
      "import": "./dist/http/templates.js"
    },
    "./http/stream-auth": {
      "types": "./dist/http/stream-auth.d.ts",
      "import": "./dist/http/stream-auth.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc && tsc -p tsconfig.ui.json",
    "build:main": "tsc",
    "build:ui": "tsc -p tsconfig.ui.json",
    "dev": "tsc --watch",
    "docs": "serve docs -p 3000",
    "test": "node --test --import tsx tests/smoke.test.ts",
    "test:watch": "vitest -c vitest.config.unit.ts",
    "test:unit": "vitest run -c vitest.config.unit.ts",
    "test:int": "vitest run -c vitest.config.int.ts",
    "test:full": "vitest run -c vitest.config.ts",
    "typecheck": "tsc --noEmit && tsc -p tsconfig.ui.json --noEmit"
  },
  "keywords": [
    "server",
    "rpc",
    "streaming",
    "multi-protocol",
    "http",
    "websocket",
    "grpc",
    "tcp"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@grpc/grpc-js": "^1.14.0",
    "@grpc/proto-loader": "^0.7.11",
    "ajv-formats": "^3.0.1",
    "graphql": "^16.11.0",
    "js-yaml": "^4.1.1",
    "pino": "^10.1.0",
    "pino-pretty": "^13.1.3",
    "ws": "^8.19.0"
  },
  "optionalDependencies": {
    "marked": "^15.0.0"
  },
  "devDependencies": {
    "@types/js-yaml": "^4.0.9",
    "serve": "^14.2.4",
    "@types/json-schema": "^7.0.15",
    "@types/node": "^25.0.3",
    "@types/ws": "^8.18.1",
    "ajv": "^8.17.1",
    "fastest-validator": "^1.19.0",
    "joi": "^17.13.3",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.16",
    "yup": "^1.6.1",
    "zod": "^4.3.5",
    "zod-to-json-schema": "^3.25.1"
  },
  "peerDependencies": {
    "ajv": "^8.0.0",
    "fastest-validator": "^1.17.0",
    "joi": "^17.0.0",
    "yup": "^1.0.0",
    "zod": "^3.0.0 || ^4.0.0",
    "zod-to-json-schema": "^3.20.0"
  },
  "peerDependenciesMeta": {
    "ajv": {
      "optional": true
    },
    "fastest-validator": {
      "optional": true
    },
    "joi": {
      "optional": true
    },
    "yup": {
      "optional": true
    },
    "zod": {
      "optional": true
    },
    "zod-to-json-schema": {
      "optional": true
    }
  }
}
